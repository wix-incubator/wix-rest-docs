          "response": ["{\"statuses\": [{\n  \"resourceId\": \"0effc651-f74d-4969-b548-29ea0290aa62\",\n  \"calendar\": \"GOOGLE\",\n  \"status\": \"CONNECTED\"\n}]} "],
