        {
      "name": "Activate Loyalty Program",
      "methodName": "ActivateLoyaltyProgram",
      "serviceName": "LoyaltyPrograms",
      "tag": "Program",
      "permissions": [      {
        "name": "Manage Loyalty",
        "description": "Request permission to manage the site's installed Wix Loyalty Program, including program and accounts."
      }],
      "events": [],
      "labels": [],
      "nameParams": [],
      "params": [],
      "ret":       {
        "type":         {
          "name": "ActivateLoyaltyProgram",
          "statusCode": "200",
          "typeParams": ["All APIs.Program._com_wixpress_loyalty_program_ActivateLoyaltyProgramResponse"]
        },
        "doc": ""
      },
      "locations": [],
      "docs":       {
        "summary": " Changes the program status to `ACTIVE`.\n\nWhen a loyalty program is installed it starts in `DRAFT` status. Customers cannot earn or redeem points while the program is not `ACTIVE`.\n \n",
        "description": [],
        "request": "POST",
        "url": "https://www.wixapis.com/loyalty-programs/v1/program/activate",
        "links": [],
        "examples": [        {
          "title": "Activate loyalty program",
          "body": ["curl -X POST \\\n  'https://www.wixapis.com/loyalty-programs/v1/program/activate' \\\n  -H 'Authorization: <AUTH>'"],
          "response": ["{\"loyaltyProgram\": {\n  \"name\": \"Star Loyalty Program\",\n  \"pointDefinition\":   {\n    \"customName\": \"Stars\",\n    \"icon\":     {\n      \"id\": \"\",\n      \"url\": \"shapes/39dce0a5d1ce498f95526b1390eaf585.svg\",\n      \"height\": 0,\n      \"width\": 0\n    }\n  },\n  \"status\": \"ACTIVE\",\n  \"createdDate\": \"2022-05-22T08:18:32.186Z\",\n  \"updatedDate\": \"2022-05-25T10:36:32.430Z\"\n}} "],
          "type": "curl",
          "extra": {}
        }],
        "extra": {}
      },
      "extra":       {
        "hasRest": true,
        "rpcRequestName": "ActivateLoyaltyProgramRequest",
        "rpcResponseName": "ActivateLoyaltyProgramResponse"
      },
      "customLabels": [{"id": "maturity-beta"}]
    },
